{
  "name": "backend",
  "version": "1.0.0",
  "description": "ShadCN Component Aggregator Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:crawler": "tsx watch src/crawler/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:reset": "tsx src/db/reset.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "postgres": "^3.4.3",
    "zod": "^3.22.4",
    "crawlee": "^3.5.8",
    "playwright": "^1.40.1",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "fuse.js": "^7.0.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "supertest": "^6.3.3",
    "drizzle-kit": "^0.20.6",
    "tsconfig-paths": "^4.2.0"
  },
  "type": "commonjs"
}